{
  "manifest_version": 2,
  "version": "0.1.0",
  "name": "Treeify",
  "description": "メモ、タブ、ブックマークをまとめてスマートに管理",
  "background": {
    "page": "Background/index.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "app-icon/19px.png"
    },
    "default_title": "Treeify",
    "default_popup": "BrowserAction/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "js": [
        "ContentScript/entryPoint.js"
      ],
      "run_at": "document_start"
    }
  ],
  "commands": {
    "open-treeify-window": {
      "suggested_key": {
        "default": "Ctrl + B"
      },
      "description": "Treeifyウィンドウを開く（最前面化）"
    }
  },
  "icons": {
    "16": "app-icon/16px.png",
    "48": "app-icon/48px.png",
    "128": "app-icon/128px.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs"
  ]
}